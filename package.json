{
    "name": "workspace",
    "private": true,
    "version": "1.0.0",
    "type": "module",
    "scripts": {
      "down": "docker compose --env-file ./server/.env  -f ./docker-compose.yaml down",
      "dev": "docker compose --env-file ./server/.env  -f ./docker-compose.yaml up",
      "dev-quite": "docker compose --env-file ./server/.env  -f ./docker-compose.yaml up -d",
      "build": "docker compose --env-file ./server/.env  -f ./docker-compose.yaml build",
      "server-restart": "docker compose --env-file ./server/.env  -f ./docker-compose.yaml restart server"
    }
  }
  